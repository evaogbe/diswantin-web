{
  "name": "diswantin",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "build": "run-s build:*",
    "build:rr": "react-router build",
    "build:server": "tsx ./scripts/build-server.ts",
    "start": "cross-env NODE_OPTIONS='--import=./dist/monitoring.js' NODE_ENV=production node dist/index.js",
    "start:test": "cross-env NODE_ENV=test PORT=8811 node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typegen": "react-router typegen",
    "typecheck": "npm run typegen && tsc --build --noEmit",
    "spellcheck": "cspell \"**\"",
    "pretest": "npm run test:setup",
    "test": "run-p \"test:unit -- --run\" test:e2e",
    "test:unit": "cross-env NODE_ENV=test vitest",
    "pretest:e2e": "cross-env NODE_ENV=test npm run build",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "test:setup": "tsx ./scripts/create-test-db.ts",
    "validate": "npm-run-all -p format:check lint typecheck spellcheck -s test",
    "init": "run-p init:*",
    "init:db": "docker-compose up -d && npm run db:migrate",
    "init:test": "playwright install --with-deps"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@fontsource-variable/nunito-sans": "^5.1.1",
    "@material-design-icons/svg": "^0.14.14",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@react-hook/resize-observer": "^2.0.2",
    "@react-router/express": "^7.1.5",
    "@react-router/node": "^7.1.5",
    "@sentry/node": "^9.0.1",
    "@sentry/profiling-node": "^9.0.1",
    "@sentry/react": "^9.0.1",
    "arctic": "^3.2.4",
    "class-variance-authority": "^0.7.1",
    "close-with-grace": "^2.2.0",
    "cmdk": "^1.0.4",
    "compression": "^1.7.5",
    "conform-to-valibot": "^1.14.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.2",
    "es-toolkit": "^1.32.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "get-port": "^7.1.0",
    "helmet": "^8.0.0",
    "isbot": "^5.1.22",
    "lucide-react": "^0.475.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^7.1.3",
    "react-virtuoso": "^4.12.5",
    "remix-themes": "^2.0.4",
    "remix-utils": "^8.1.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "uid": "^2.0.2",
    "use-debounce": "^10.0.4",
    "valibot": "^1.0.0-rc.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@playwright/test": "^1.50.1",
    "@react-router/dev": "^7.1.5",
    "@sentry/vite-plugin": "^3.1.2",
    "@tailwindcss/vite": "^4.0.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.1",
    "@types/pg": "^8.11.11",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "cross-env": "^7.0.3",
    "cspell": "^8.17.3",
    "drizzle-kit": "^0.30.4",
    "esbuild": "0.25.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest-extended": "^4.0.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-env-only": "^3.0.3",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "overrides": {
    "valibot": "$valibot"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
