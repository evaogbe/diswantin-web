{
  "name": "diswantin",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "build": "run-s build:*",
    "build:rr": "react-router build",
    "build:server": "tsx ./scripts/build-server.ts",
    "start": "cross-env NODE_OPTIONS='--import=./dist/monitoring.js' NODE_ENV=production node dist/index.js",
    "start:test": "cross-env NODE_ENV=test PORT=8811 node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "react-router typegen && tsc",
    "spellcheck": "cspell \"**\"",
    "pretest": "cross-env NODE_ENV=test run-s test:setup build",
    "test": "cross-env NODE_ENV=test playwright test",
    "test:setup": "tsx ./scripts/setup-test-server.ts",
    "validate": "run-p format:check lint typecheck spellcheck test",
    "init": "run-p init:*",
    "init:db": "docker-compose up -d && npm run db:migrate",
    "init:test": "playwright install --with-deps"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@fontsource-variable/nunito-sans": "^5.1.1",
    "@material-design-icons/svg": "^0.14.13",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@react-router/express": "^7.1.3",
    "@react-router/node": "^7.1.3",
    "@sentry/node": "^8.50.0",
    "@sentry/profiling-node": "^8.50.0",
    "@sentry/react": "^8.50.0",
    "arctic": "^3.1.3",
    "class-variance-authority": "^0.7.1",
    "close-with-grace": "^2.2.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "compression": "^1.7.5",
    "conform-to-valibot": "^1.12.2",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "get-port": "^7.1.0",
    "helmet": "^8.0.0",
    "isbot": "^5.1.21",
    "lucide-react": "^0.473.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^7.1.3",
    "remix-themes": "^2.0.4",
    "remix-utils": "^8.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uid": "^2.0.2",
    "use-debounce": "^10.0.4",
    "valibot": "^1.0.0-beta.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@playwright/test": "^1.49.1",
    "@react-router/dev": "^7.1.3",
    "@sentry/vite-plugin": "^3.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.10",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cspell": "^8.17.2",
    "drizzle-kit": "^0.30.2",
    "esbuild": "0.24.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.9",
    "vite-env-only": "^3.0.3",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "overrides": {
    "valibot": "^1.0.0-beta.12"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
