{
  "name": "diswantin",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "tsx ./src/index.ts",
    "build": "run-s build:*",
    "build:remix": "remix vite:build",
    "build:server": "tsx ./scripts/build-server.ts",
    "start": "cross-env NODE_OPTIONS='--import=./dist/monitoring.js' NODE_ENV=production node dist/index.js",
    "start:test": "cross-env NODE_ENV=test PORT=8811 node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "spellcheck": "cspell \"**\"",
    "pretest": "cross-env NODE_ENV=test run-s test:setup build",
    "test": "cross-env NODE_ENV=test playwright test",
    "test:setup": "tsx ./scripts/setup-test-server.ts",
    "validate": "run-p format:check lint typecheck spellcheck test",
    "init": "run-p init:*",
    "init:db": "docker-compose up -d && npm run db:migrate",
    "init:test": "playwright install --with-deps"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@remix-run/express": "^2.14.0",
    "@remix-run/node": "^2.14.0",
    "@remix-run/react": "^2.14.0",
    "@sentry/remix": "^8.39.0",
    "arctic": "^2.2.2",
    "close-with-grace": "^2.1.0",
    "compression": "^1.7.5",
    "conform-to-valibot": "^1.11.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.3",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "get-port": "^7.1.0",
    "helmet": "^8.0.0",
    "isbot": "^5.1.17",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.7.0",
    "uid": "^2.0.2",
    "valibot": "^1.0.0-beta.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@playwright/test": "^1.49.0",
    "@remix-run/dev": "^2.14.0",
    "@remix-run/route-config": "^2.14.0",
    "@sentry/vite-plugin": "^2.22.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "cspell": "^8.16.0",
    "drizzle-kit": "^0.28.1",
    "esbuild": "0.24.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.4.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "glob": "^11.0.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.38",
    "prettier": "3.4.1",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^6.0.1",
    "vite-env-only": "^3.0.3",
    "vite-tsconfig-paths": "^5.1.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
